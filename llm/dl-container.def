Bootstrap: docker
From: nvidia/cuda:12.4.1-cudnn-devel-ubuntu22.04

%environment
  export APPTAINER_CACHEDIR=/tmp/apptainer_cache

%files 
  requirements.txt requirements.txt
  qDora.py qDora.py

%post
    apt-get update
    apt-get install -y python3 python3-pip
    pip3 install -Ur requirements.txt 
